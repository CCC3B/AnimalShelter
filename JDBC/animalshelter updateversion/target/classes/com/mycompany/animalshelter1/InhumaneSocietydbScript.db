BEGIN TRANSACTION;
CREATE TABLE IF NOT EXISTS "EmployeeTable" (
	"EmployeeNumber"	INTEGER NOT NULL UNIQUE,
	"Sex"	TEXT,
	"Name"	TEXT,
	"Last"	TEXT,
	"Age"	INTEGER,
	"Address"	TEXT,
	"Social"	INTEGER,
	"Animals"	INTEGER,
	"Level"	TEXT,
	PRIMARY KEY("EmployeeNumber")
);
CREATE TABLE IF NOT EXISTS "AdoptionTable" (
	"Number"	INTEGER NOT NULL UNIQUE,
	"Sex"	TEXT NOT NULL,
	"Species"	TEXT NOT NULL,
	"Breed"	TEXT NOT NULL,
	"Age"	INTEGER NOT NULL,
	"Name"	TEXT NOT NULL,
	"Temper"	INTEGER,
	"Adoptable"	INTEGER,
	"Username"	TEXT,
	FOREIGN KEY("Username") REFERENCES "EmployeeTable"("EmployeeNumber"),
	PRIMARY KEY("Number")
);
CREATE TABLE IF NOT EXISTS "Credentials" (
	"Username"	TEXT,
	"Password"	TEXT,
	"ID"	INTEGER,
	FOREIGN KEY("Username") REFERENCES "EmployeeTable"("EmployeeNumber"),
	PRIMARY KEY("Username")
);
CREATE TABLE IF NOT EXISTS "home" (
	"number"	integer,
	"sex"	text NOT NULL,
	"species"	text NOT NULL,
	"breed"	text NOT NULL,
	PRIMARY KEY("number")
);
CREATE TABLE IF NOT EXISTS "LogTable" (
	"LogID"	INTEGER NOT NULL,
	"Time"	INTEGER NOT NULL,
	"SQL"	TEXT
);

CREATE TRIGGER deletelogBEF
AFTER DELETE ON AdoptionTable
BEGIN
	Insert into LogTable(LogID, time, sql) 
	values("", datetime('now'), "User is DELETING... " || OLD.number || '/' || OLD.species || '/' || OLD.breed || '/' || OLD.age || '/' || OLD.name || '/' || OLD.temper ||
	'/' || OLD.adoptable || '/' || OLD.username );
END;
CREATE TRIGGER addlogAFT
AFTER INSERT ON AdoptionTable
BEGIN
	Insert into LogTable(LogID, time, sql) 
	values("", datetime('now')," User INSERTED: " || NEW.number || '/' || NEW.species || '/' || NEW.breed || '/' || NEW.age || '/' || NEW.name || '/' || NEW.temper ||
	'/' || NEW.adoptable);
END;

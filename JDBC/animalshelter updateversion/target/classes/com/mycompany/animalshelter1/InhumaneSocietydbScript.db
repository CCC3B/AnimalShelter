CREATE TRIGGER validlog
   BEFORE INSERT ON AdoptionTable
BEGIN
   SELECT
      CASE
        WHEN NEW.Username NOT LIKE '%1__%.__%'     THEN   RAISE (ABORT,'Invalid Username need to start with 1')
        WHEN NOT typeof(NEW.number) = 'integer'  THEN   RAISE (ABORT,'Invalid number')
    END;       
       
END;
COMMIT;
